function get_zones(region)
{
	region_id = region.split(',');
	$.post('ajax_page.php',{'task':'getZone','region':region_id[1]},function(data){
		var html = '<option value="">Select</option>';
		$('#district').html(html);
		for(i=0;i<data.length;i++)
		{
			html+= "<option value='"+data[i].id+"'>"+data[i].title_en+"</option>";
		}
		$('#zone').html(html);
	},'json');
}
function get_districts(zone_id)
{
	$.post('ajax_page.php',{'task':'getDistrict','zone':zone_id},function(data){
		var html = '<option value="">Select</option>';
		for(i=0;i<data.length;i++)
		{
			html+= "<option value='"+data[i].id+"'>"+data[i].title_en+"</option>";
		}
		$('#district').html(html);
	},'json');
}
</script>			
