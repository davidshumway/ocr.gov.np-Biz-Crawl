		{name:'registration_date',index:'registration_date', sorttype:"date",width:'110'}, 
		{name:'name_eng',index:'name_eng',width:'250'}, 
//		{name:'name',index:'name',width:'175'}, 
		{name:'type_name',index:'type_name',width:'150'}, 
		{name:'address',index:'address',width:'235'}
		//{name:'objective',index:'objective',width:'250'}
		], 
	pager: "#plist47", 
	viewrecords: true,
	caption:"Search Results" 
	}); 
   $('#form_search').validate();
   //$('.date').datepicker({dateFormat:'yy-mm-dd'}); 
      $('#div_search').hide();
   });
function get_zones(region)
{
	region_id = region.split(',');
	$.post('ajax_page.php',{'task':'getZone','region':region_id[1]},function(data){
		var html = '<option value="">Select</option>';
		$('#district').html(html);
		for(i=0;i<data.length;i++)
		{
			html+= "<option value='"+data[i].id+"'>"+data[i].title_en+"</option>";
		}
		$('#zone').html(html);
	},'json');
}
function get_districts(zone_id)
{
	$.post('ajax_page.php',{'task':'getDistrict','zone':zone_id},function(data){
		var html = '<option value="">Select</option>';
		for(i=0;i<data.length;i++)
		{
			html+= "<option value='"+data[i].id+"'>"+data[i].title_en+"</option>";
		}
		$('#district').html(html);
	},'json');
}
</script>			
