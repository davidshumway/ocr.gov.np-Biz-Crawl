}

/* Sub level menu links style */
.suckerdiv ul li a{
display: block;
overflow: auto; /*force hasLayout in IE7 */
color: #ED5464;
text-decoration: none;
background: #fff;
padding: 5px 5px;
border-bottom:1px solid #E9E9E9;
}

