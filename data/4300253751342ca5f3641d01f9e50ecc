                      <script>
function validateNSubmit(form)
{
	if(document.getElementById('email').value.length > 0)
	{
		if(validateEmail())
		{
			addSubscribe(form);
			return false;
		}
		else
		{
			document.getElementById('email').focus();
			return false;
		}
	}
	else
	{
		alert("Email address should be provided.");
		document.getElementById('email').focus();
		return false;
	}
}

function validateEmail()
{
	pattern="^\\w{1,}@\\w{1,}(\\.\\w{1,}){1,}$";
	if(!document.getElementById('email').value.match(pattern))
	{
		alert("Please enter valid email address.");
		document.getElementById('email').focus();
		return false; 
	}	
	return true;
}
</script>    
